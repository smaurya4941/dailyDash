{
    
    "builds": [
        {
            "src": "myproject/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb","runtime":"python3.10.7"}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "myproject/wsgi.py"
        }
    ]
}